{
  "onnx_path": "/opt/models/yolov8s-pose.onnx",
  "target_path": "/opt/models/yolov8s-pose.engine",
  "model_information": {
    "name": "YOLOv8s Pose",
    "description": "YOLOv8s model for pose estimation",
    "version": "1.0.0",
    "author": "Orlando Chen",
    "date": "2023-10-01",
    "maximum_batch_size": 8,
    "input_image_type": "BGR",
    "input_tensors": [
      {
        "name": "input",
        "type": "float32",
        "description": "Input tensor for the model",
        "format": "NCHW",
        "dimensions": [1, 3, 640, 640]
      }
    ],
    "output_tensors": [
      {
        "name": "output0",
        "type": "float32",
        "description": "Output tensor for the model",
        "format": "NFS",
        "dimensions": [1, 56, 8400]
      }
    ]
  },
  "trt_config": {
    "precision": "fp16",
    "min_shapes": "images:1x3x640x640",
    "opt_shapes": "images:4x3x640x640",
    "max_shapes": "images:8x3x640x640",
    "use_nvdla": false,
    "nvdla_core_id": 1,
    "verbose": true
  }
}
